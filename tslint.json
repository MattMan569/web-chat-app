{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [true, {
            "singleline": "never",
            "multiline": "always"
        }],
        "max-line-length":false,
        "no-consecutive-blank-lines":true,
        "semicolon":true,
        "prefer-template":true,
        "prefer-const":true,
        "no-console":false,
        "no-reference":false,
        "no-var-requires":false
    },
    "rulesDirectory": []
}